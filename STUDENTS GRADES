let studentsScore = [
    {
        name: 'Andi',
        score: 90
    },
    {
        name: 'Rudi',
        score: 80
    },
    {
        name: 'Dira',
        score: 100
    }
];

// how to access the students data
studentsScore.forEach(student => {
    console.log(`${student.name} scored ${student.score}`);
});
